# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Nursing' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Nursing

  target 'NursingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NursingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'NursingCommon' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NursingCommon
  pod 'SwiftMoment', '~> 0.7'

  target 'NursingCommonTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
